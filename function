#return function
def sum(x,y):
    print("krishnendu")
    result=x+y
    return result
print("krishnendu")
print(sum(11,11))

#arbitary argument
def add_numbers(*data):
    total = 0
    for num in data:
        total += num
    print(f"The total is: {total}")

add_numbers(1, 2, 3)
add_numbers(4, 5, 6, 7, 8)


